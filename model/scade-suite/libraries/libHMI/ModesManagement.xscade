								<!-- _L1 = MODE_AUTO; -->
								<Equation>
									<lefts>
										<VariableRef name="_L1"/>
									</lefts>
									<right>
										<IdExpression>
											<path>
												<ConstVarRef name="MODE_AUTO"/>
											</path>
										</IdExpression>
									</right>
									<pragmas>
										<ed:Equation oid="!ed/620e/33E8/42A0/5a2020af7a84"/>
									</pragmas>
								</Equation>
								<!-- mode = _L1; -->
								<Equation>
									<lefts>
										<VariableRef name="mode"/>
									</lefts>
									<right>
										<IdExpression>
											<path>
												<ConstVarRef name="_L1"/>
											</path>
										</IdExpression>
									</right>
									<pragmas>
										<ed:Equation oid="!ed/620f/33E8/42A0/5a2020af7a44"/>
									</pragmas>
								</Equation>
							</data>
							<pragmas>
								<ed:State oid="!ed/620d/33E8/42A0/5a2020af585e"/>
							</pragmas>
						</State>
						<State name="ON">
							<unless>
								<Transition kind="restart">
									<target>
										<StateRef name="State5"/>
									</target>
									<condition>
										<!-- pressed and inModeArea and  not inBtnArea -->
										<NAryOp name="6" operator="and">
											<operands>
												<IdExpression>
													<path>
														<ConstVarRef name="pressed"/>
													</path>
												</IdExpression>
												<IdExpression>
													<path>
														<ConstVarRef name="inModeArea"/>
													</path>
												</IdExpression>
												<!--  not inBtnArea -->
												<UnaryOp name="1" operator="not">
													<operand>
														<IdExpression>
															<path>
																<ConstVarRef name="inBtnArea"/>
															</path>
														</IdExpression>
													</operand>
												</UnaryOp>
											</operands>
										</NAryOp>
									</condition>
									<pragmas>
										<ed:Transition oid="!ed/6216/33E8/42A0/5a2020af78da"/>
									</pragmas>
								</Transition>
								<Transition kind="restart">
									<target>
										<StateRef name="State6"/>
									</target>
									<condition>
										<!-- pressed and inBtnArea -->
										<NAryOp name="4" operator="and">
											<operands>
												<IdExpression>
													<path>
														<ConstVarRef name="pressed"/>
													</path>
												</IdExpression>
												<IdExpression>
													<path>
														<ConstVarRef name="inBtnArea"/>
													</path>
												</IdExpression>
											</operands>
										</NAryOp>
									</condition>
									<pragmas>
										<ed:Transition oid="!ed/6217/33E8/42A0/5a2020af5506"/>
									</pragmas>
								</Transition>
							</unless>
							<locals>
								<Variable name="_L2">
									<type>
										<NamedType>
											<type>
												<TypeRef name="int32"/>
											</type>
										</NamedType>
									</type>
									<pragmas>
										<ed:Variable oid="!ed/6215/33E8/42A0/5a2020af3dab"/>
									</pragmas>
								</Variable>
							</locals>
							<data>
								<!-- mode = _L2; -->
								<Equation>
									<lefts>
										<VariableRef name="mode"/>
									</lefts>
									<right>
										<IdExpression>
											<path>
												<ConstVarRef name="_L2"/>
											</path>
										</IdExpression>
									</right>
									<pragmas>
										<ed:Equation oid="!ed/6213/33E8/42A0/5a2020af6ee2"/>
									</pragmas>
								</Equation>
								<!-- _L2 = MODE_ON; -->
								<Equation>
									<lefts>
										<VariableRef name="_L2"/>
									</lefts>
									<right>
										<IdExpression>
											<path>
												<ConstVarRef name="MODE_ON"/>
											</path>
										</IdExpression>
									</right>
